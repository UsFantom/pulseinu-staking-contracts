{
  "version": "1.0.0",
  "devDependencies": {
    "@babel/preset-typescript": "^7.18.6",
    "@codechecks/client": "^0.1.12",
    "@commitlint/cli": "^14.1.0",
    "@commitlint/config-conventional": "^14.1.0",
    "@ethersproject/abi": "^5.5.0",
    "@ethersproject/abstract-signer": "^5.5.0",
    "@ethersproject/bignumber": "^5.5.0",
    "@ethersproject/bytes": "^5.5.0",
    "@ethersproject/providers": "^5.5.0",
    "@gnosis.pm/safe-core-sdk": "3.1.1",
    "@gnosis.pm/safe-core-sdk-types": "1.6.1",
    "@gnosis.pm/safe-ethers-lib": "1.6.1",
    "@gnosis.pm/safe-service-client": "^1.3.1",
    "@nomicfoundation/hardhat-chai-matchers": "^1.0.6",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.8",
    "@nomicfoundation/hardhat-toolbox": "^2.0.2",
    "@nomiclabs/hardhat-ethers": "^2.0.2",
    "@types/chai": "^4.3.4",
    "@types/fs-extra": "^9.0.13",
    "@types/mocha": "^10.0.0",
    "@types/node": "^16.11.10",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "axios": "^0.27.2",
    "chai": "^4.3.7",
    "commitizen": "^4.2.4",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "dotenv": "^10.0.0",
    "eslint": "^8.3.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^27.2.1",
    "ethers": "5.6.9",
    "fs-extra": "^10.0.0",
    "hardhat": "latest",
    "hardhat-deploy": "^0.11.25",
    "hardhat-deploy-ethers": "next",
    "hardhat-gas-reporter": "^1.0.8",
    "husky": "^7.0.4",
    "lint-staged": "^11.2.6",
    "lodash": "^4.17.21",
    "mocha": "^10.1.0",
    "pinst": "^2.1.6",
    "prettier": "^2.5.0",
    "prettier-plugin-solidity": "^1.0.0-beta.19",
    "shelljs": "^0.8.4",
    "shx": "^0.3.3",
    "solhint": "^3.3.6",
    "solhint-plugin-prettier": "^0.0.5",
    "solidity-coverage": "^0.8.3",
    "ts-generator": "^0.1.1",
    "ts-node": "^10.4.0",
    "typechain": "^8.2.0",
    "typescript": "^4.5.2"
  },
  "files": [
    "/contracts"
  ],
  "keywords": [
    "blockchain",
    "ethereum",
    "hardhat",
    "smart-contracts",
    "solidity"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "clean": "shx rm -rf ./artifacts ./cache ./coverage ./src/types ./coverage.json",
    "commit": "git-cz",
    "compile": "cross-env TS_NODE_TRANSPILE_ONLY=true hardhat compile",
    "coverage": "yarn typechain && hardhat coverage --solcoverjs ./.solcover.js --temp artifacts --testfiles \"./test/**/*.ts\"",
    "deploy": "hardhat deploy:Greeter",
    "lint": "yarn lint:sol && yarn lint:ts && yarn prettier:check",
    "lint:sol": "solhint --config ./.solhint.json --max-warnings 0 \"contracts/**/*.sol\"",
    "lint:ts": "eslint --config ./.eslintrc.yaml --ignore-path ./.eslintignore --ext .js,.ts .",
    "postinstall": "husky install",
    "postpublish": "pinst --enable",
    "prepublishOnly": "pinst --disable",
    "prettier": "prettier --config ./.prettierrc.yaml --write \"**/*.{js,json,md,sol,ts}\"",
    "prettier:check": "prettier --check --config ./.prettierrc.yaml \"**/*.{js,json,md,sol,ts}\"",
    "test:pulseinu": "hardhat test  ./test/pulseinu.ts",
    "test:token-burner": "hardhat test  ./test/token-burner.ts",
    "test:staking-pool": "hardhat test  ./test/staking-pool.ts",
    "test:staking-pool:pulseTest": "hardhat test --config hardhat.config.pulseTest.ts ./test/staking-pool.ts",
    "test:deployer": "hardhat test  ./test/deployer.ts",
    "typechain": "cross-env TS_NODE_TRANSPILE_ONLY=true hardhat typechain",
    "toolbox": "sudo docker run -it --rm -v $PWD:/share eth-security-toolbox",
    "slither:locker": "slither /share --checklist --config-file /share/slither.config.json"
  },
  "dependencies": {
    "@chainlink/contracts": "^0.4.1",
    "@nomiclabs/hardhat-etherscan": "latest",
    "@openzeppelin/contracts": "^4.7.0",
    "@pancakeswap/pancake-swap-lib": "^0.0.4",
    "@typechain/ethers-v5": "^11.0.0",
    "@typechain/hardhat": "^7.0.0",
    "solc": "0.8.18"
  },
  "packageManager": "yarn@3.5.0"
}
